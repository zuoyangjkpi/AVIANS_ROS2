    <!-- 行走人员 - 不规则样条曲线路径 -->
    <actor name="walking_person">
      <skin>
        <filename>model://actor/meshes/walk.dae</filename>
        <scale>1.0</scale>
      </skin>
      <animation name="walking">
        <filename>model://actor/meshes/walk.dae</filename>
        <interpolate_x>true</interpolate_x>
      </animation>
      <script>
        <loop>true</loop>
        <delay_start>3.0</delay_start>
        <auto_start>true</auto_start>
        <!-- 使用更高的tension值创建平滑的样条曲线路径 -->
        <trajectory id="0" type="walking" tension="0.8">
            <!-- 起点 -->
            <waypoint>
                <time>0</time>
                <pose>0 0 1.0 0 0 0</pose>
            </waypoint>
            <!-- 向东北方向 -->
            <waypoint>
                <time>5</time>
                <pose>2.5 2 1.0 0 0 0.6</pose>
            </waypoint>
            <!-- 转向东南 -->
            <waypoint>
                <time>10</time>
                <pose>4 0 1.0 0 0 -0.8</pose>
            </waypoint>
            <!-- 向南移动 -->
            <waypoint>
                <time>15</time>
                <pose>3 -3 1.0 0 0 -2.2</pose>
            </waypoint>
            <!-- 转向西南 -->
            <waypoint>
                <time>20</time>
                <pose>0 -4 1.0 0 0 3.0</pose>
            </waypoint>
            <!-- 向西北方向 -->
            <waypoint>
                <time>25</time>
                <pose>-3 -3 1.0 0 0 2.4</pose>
            </waypoint>
            <!-- 转向北方 -->
            <waypoint>
                <time>30</time>
                <pose>-4 0 1.0 0 0 1.6</pose>
            </waypoint>
            <!-- 向东北返回 -->
            <waypoint>
                <time>35</time>
                <pose>-2.5 2.5 1.0 0 0 0.8</pose>
            </waypoint>
            <!-- 转向东方 -->
            <waypoint>
                <time>40</time>
                <pose>0 3.5 1.0 0 0 0.2</pose>
            </waypoint>
            <!-- 曲线返回起点 -->
            <waypoint>
                <time>45</time>
                <pose>2 2 1.0 0 0 -0.6</pose>
            </waypoint>
            <waypoint>
                <time>50</time>
                <pose>0 0 1.0 0 0 0</pose>
            </waypoint>
        </trajectory>
      </script>

      <!-- Actor pose publisher plugin -->
      <plugin filename="gz-sim8-pose-publisher-system" name="gz::sim::systems::PosePublisher">
        <publish_nested_model_pose>true</publish_nested_model_pose>
        <publish_link_pose>true</publish_link_pose>
        <publish_collision_pose>false</publish_collision_pose>
